{
    "compilerOptions": {
    "outDir": "./dist",
    "noImplicitAny": true,
    "allowJs": true,
    "target": "ESNext",
    "removeComments": true,
    "sourceMap": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true
  },
    "include": [
        "./src/**/*",
        "./src/test/fixture/*.*"
    ],
    "exclude": [
        "./node_modules",
        "./dist"
    ]
}
